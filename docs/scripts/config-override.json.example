    "connectorId": "<IN connector id>",
    "externalKey": "<IN external key>",
    "connectorConfig": {
      "unit_id": "<AIR unit id>",
      "credentials": {
        "clientId": "<AIR client id>",
        "secret": "<AIR client secret>"
      },
      "connection_url": "https://connect.sandbox.uk.eagleeye.com/in/my-platform/<IN connector id>",
      "platform": {
        "id": "",
        "name": "my-platform",
